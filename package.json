{
  "name": "catastrophee",
  "version": "0.0.1",
  "description": "Reusable components, models and utils for react and mobx apps",
  "main": "dist/lib/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "start": "yarn run storybook",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "bundle": "webpack --config config/webpack.config.dist.js --output-library=rio",
    "build": "lerna run build",
    "build-storybook": "build-storybook -s ./public",
    "build:ci": "lerna exec --scope=catastropheewebsite yarn build",
    "test": "tsc && npm run build-schema && jest",
    "jest": "jest",
    "stats": "webpack --mode production ./src --profile --json > dist/stats.json; webpack-bundle-analyzer dist/stats.json",
    "prepublishOnly": "npm run build",
    "reversion": "chmod 777 reversion.js | node reversion.js",
    "rebuild": "lerna run tsc",
    "publishStoryBook": "git add storybook-static/ && git commit -m 'Update catastrophee docs' && git push origin mater && git add . && git stash",
    "publishPage": "yarn run build-storybook && yarn run publishStoryBook",
    "publish": "lerna publish && node ./scripts/generatePackageVersions.js && git add . && git commit -m 'Update package version' && git push origin master && yarn run publishPage"
  },
  "engines": {
    "node": ">=10.13.0 <11.0.0"
  },
  "author": "Stephani Bishop",
  "license": "uicommon",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-regular-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@types/react": "^16.8.20",
    "@types/react-dom": "^16.8.4",
    "axios": "^0.18.0",
    "codemirror": "^5.47.0",
    "dompurify": "^1.0.10",
    "glamor": "^2.20.40",
    "js-cookie": "^2.2.0",
    "jspdf": "1.5.3",
    "lodash": "^4.17.11",
    "mobx": "^5.9.4",
    "mobx-react": "^5.4.3",
    "mobx-utils": "^5.2.0",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "storybook-readme": "^5.0.5",
    "uuidv4": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-actions": "^5.1.8",
    "@storybook/addon-info": "^5.1.8",
    "@storybook/addon-links": "^5.1.8",
    "@storybook/addon-storysource": "^5.1.8",
    "@storybook/addons": "^5.1.8",
    "@storybook/react": "5.2.0-alpha.29",
    "@types/jest": "^24.0.12",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.0",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-loader": "^8.0.5",
    "jest": "^24.7.1",
    "lerna": "^3.13.4",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-scripts": "^2.1.8",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.0.3",
    "typescript": "^3.4.5",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.0"
  }
}
